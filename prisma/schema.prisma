generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model star {
  id           Int     @id(map: "stars_pkey") @default(autoincrement())
  x            Float
  y            Float
  z            Float
  distance     Float
  officialName String? @db.VarChar
  hd           Int?
  hr           Int?
  colorIndex   Float?  @db.Real
  magnitude    Float   @db.Real
  color        String? @db.VarChar(6)
  r            Float   @db.Real
  g            Float   @db.Real
  b            Float   @db.Real
}
